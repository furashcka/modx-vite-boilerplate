{
  "overrides": [
    {
      "files": ["*.tpl", "*.svg"],
      "options": {
        "parser": "html"
      }
    },
    {
      "files": "*.tpl",
      "options": {
        "htmlWhitespaceSensitivity": "ignore",
        "bracketSameLine": true
      }
    }
  ],
  "plugins": ["prettier-plugin-tailwindcss"]
}
